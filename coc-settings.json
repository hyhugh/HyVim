{
  "suggest.enablePreview": true,
  "coc.preferences.colorSupport": false,
  "python.formatting.provider": "black",
  "signature.preferShownAbove": true,
  "python.formatting.blackPath": "/usr/local/bin/black",
  "signature.hideOnTextChange": false,
  "snippets.ultisnips.enable": true,
  "rust.racer_completion": true,
  "rust.all_targets": false,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "typescriptreact",
    "vue-html": "html",
    "razor": "html"
  },
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.jumpCommand": "edit",
  "snippets.userSnippetsDirectory": "~/HyVim/ultisnips",
  "git.addGBlameToBufferVar": true,
  "git.addGBlameToVirtualText": false,
  "git.signOffset": 1,
  "git.realtimeGutters": true,
  "git.virtualTextPrefix": "     ----- ",
  "list.normalMappings": {
    "t": "action:tabe",
    "v": "action:vsplit",
    "s": "action:split",
    "d": "action:delete"
  },
  "list.insertMappings": {
    "<C-t>": "action:tabe",
    "<C-v>": "action:vsplit",
    "<C-x>": "action:split",
    "<C-w>": "command:wincmd k"
  },
  "coc.source.issues.enable": false,
  "snippets.extends": {
    "typescript.tsx": [
      "typescriptreact",
      "typescript",
      "javascript"
    ]
  },
  "languageserver": {
    "java": {
      "command": "~/packages/java-language-server/dist/lang_server_mac.sh",
      "rootPatterns": [
        "BUILD"
      ],
      "trace.server": "verbose",
      "filetypes": [
        "java1"
      ]
    }
  },
  "yank.enableCompletion": false,
  "clangd.semanticHighlighting": true
}
